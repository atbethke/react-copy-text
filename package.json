{
  "name": "react-copy-text",
  "version": "1.0.0",
  "description": "Copy text to the clipboard like it's 1999",
  "main": "lib/CopyText.js",
  "scripts": {
    "build": "run-p build:*",
    "build:js": "babel src --out-dir lib",
    "test": "run-s lint test:*",
    "test:ava": "nyc --reporter=lcov --reporter=text ava",
    "lint": "standard",
    "clean": "rm -rf lib/**",
    "prepublishOnly": "run-s build"
  },
  "keywords": [
    "copy",
    "clipboard",
    "text",
    "string",
    "paste",
    "execCommand"
  ],
  "author": "Alan Shaw",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jsdom": "^11.5.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "standard": "^10.0.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tableflip/react-copy-text.git"
  },
  "bugs": {
    "url": "https://github.com/tableflip/react-copy-text/issues"
  },
  "homepage": "https://github.com/tableflip/react-copy-text#readme"
}
